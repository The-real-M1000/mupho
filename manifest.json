{
  "id": "mupho.vercel.app",
  "name": "Mupho",
  "short_name": "Mupho",
  "description": "Mupho, la app que combina Spotify y Pinterest.",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "background_color": "#005C53",
  "theme_color": "#005C53",
  "orientation": "portrait",
  "lang": "es",
  "display_override": ["window-controls-overlay"],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "icons": [
    {
      "src": "/icon.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icon.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshot1.png",
      "sizes": "1080x1920",
      "type": "image/png"
    },
    {
      "src": "/screenshot2.png",
      "sizes": "1080x1920",
      "type": "image/png"
    }
  ]
}
